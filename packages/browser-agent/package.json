{
  "name": "@stacksleuth/browser-agent",
  "version": "0.2.1",
  "description": "StackSleuth in-house browser automation agent for debugging and user simulation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "test:ci": "jest --coverage --ci",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "benchmark": "node benchmarks/automation-performance.js",
    "performance:test": "clinic doctor -- node benchmarks/memory-test.js"
  },
  "keywords": [
    "browser",
    "automation",
    "debugging",
    "crawling",
    "testing",
    "simulation"
  ],
  "dependencies": {
    "@stacksleuth/core": "^0.2.1",
    "playwright": "^1.40.0",
    "puppeteer": "^21.0.0",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "ts-jest": "^29.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jack-GitHub12/StackSleuth.git",
    "directory": "packages/browser-agent"
  },
  "license": "MIT"
} 