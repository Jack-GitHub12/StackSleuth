{
  "name": "@stacksleuth/laravel-agent",
  "version": "0.2.1",
  "description": "StackSleuth Laravel/PHP backend profiling agent with Eloquent ORM monitoring and middleware instrumentation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "benchmark": "node benchmarks/laravel-performance.js",
    "performance:test": "clinic doctor -- node benchmarks/memory-test.js"
  },
  "keywords": [
    "laravel",
    "php",
    "backend",
    "performance",
    "profiling",
    "monitoring",
    "eloquent"
  ],
  "dependencies": {
    "@stacksleuth/core": "^0.2.1",
    "axios": "^1.6.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "clinic": "^12.0.0",
    "benchmark": "^2.1.4",
    "eslint": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jack-GitHub12/StackSleuth.git",
    "directory": "packages/laravel-agent"
  },
  "license": "MIT"
} 